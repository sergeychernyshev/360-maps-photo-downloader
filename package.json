{
  "name": "google-streetview-downloader",
  "version": "1.0.0",
  "description": "A script to download 360 photos from Google Street View.",
  "main": "dist/webapp.js",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc -p src/public/js && cp -r src/views dist/views && cp -r src/public/css dist/public/css",
    "prestart": "npm run build",
    "start": "node dist/webapp.js",
    "dev": "nodemon",
    "stop": "pkill -f nodemon"
  },
