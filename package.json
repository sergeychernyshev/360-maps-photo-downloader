    "axios": "^1.7.2",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "googleapis": "^140.0.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "memorystore": "^1.6.7",
    "piexifjs": "^1.0.6",
    "prettier": "^3.6.2",
    "ws": "^8.17.1",
    "xmp-js": "^0.0.5"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/leaflet": "^1.9.12",
    "@types/leaflet.markercluster": "^1.5.4",
    "@types/node": "^20.12.12",
    "@types/ws": "^8.5.10",
